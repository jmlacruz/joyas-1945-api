{
  "name": "fauth-node",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-assets",
    "build:prod": "NODE_ENV=production npm run build",
    "compile": "tsc",
    "copy-assets": "node copyfiles.js",
    "clean": "rimraf dist",
    "dev": "nodemon --exec \"npm run build && npm start\" --watch src --ext ts,json,html",
    "dev:debug": "nodemon --exec \"npm run build && npm run start:debug\" --watch src --ext ts,json,html",
    "build:watch": "npm run clean && concurrently \"tsc --watch\" \"nodemon --watch dist --exec npm start\"",
    "start": "node ./dist/server.js",
    "start:debug": "node --inspect=0.0.0.0:9229 ./dist/server.js",
    "start:debug-brk": "node --inspect-brk=0.0.0.0:9229 ./dist/server.js",
    "start:prod": "NODE_ENV=production node ./dist/server.js",
    "debug": "npm run build && npm run start:debug",
    "debug:brk": "npm run build && npm run start:debug-brk",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "basic-ftp": "^5.0.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "firebase-admin": "^12.1.1",
    "getstream": "^8.4.1",
    "image-size": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "node-cron": "^4.2.1",
    "nodemon": "^3.1.4",
    "path": "^0.12.7",
    "sharp": "^0.33.4",
    "stream": "^0.0.3",
    "stream-chat": "^8.37.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "fs-extra": "^11.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
